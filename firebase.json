{
  "hosting": {
    "public": "./www",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/#Botris",
        "destination": "/archive",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      },
      {
        "source": "opensearch.xml",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/xml; charset=utf-8"
          },
          {
            "key": "Cache-Control",
            "value": "max-age=86400, must-revalidate, private"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      },
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=300"
          }
        ]
      }
    ]
  }
}
