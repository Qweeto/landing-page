{
  "hosting": {
    "public": "./www",
    "ignore": [
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/archive",
        "destination": "/archive.html"
      }
    ],
    "headers": [
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=300"
          }
        ]
      }
    ]
  }
}
