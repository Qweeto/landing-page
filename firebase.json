{
  "hosting": {
    "public": "./www",
    "ignore": [
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/archive.html",
        "destination": "/archive",
        "type": 301
      },
      {
        "source": "/mission.html",
        "destination": "/mission",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=300"
          }
        ]
      }
    ]
  }
}
